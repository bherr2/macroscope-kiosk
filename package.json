{
  "name": "macroscope-kiosk",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:staging": "node --max_old_space_size=3000 ./node_modules/.bin/ng build --prod --base-href=/macroscope-kiosk--staging/",
    "build:production": "node --max_old_space_size=3000 ./node_modules/.bin/ng build --prod --base-href=/macroscope-kiosk/",
    "build:release": "node --max_old_space_size=3000 ./node_modules/.bin/ng build --prod --base-href=./",
    "build:kiosk": "node ./build-scripts/build-macroscope-kiosk.js",
    "compodoc": "npx compodoc -p src/tsconfig.app.json --output dist/doc --includes architecture -a architecture",
    "release": "npm run build:kiosk && npm run build:release && cd ./dist && zip -r release/macroscope-kiosk-site.zip macroscope-kiosk",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.3",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "~7.2.1",
    "@angular/compiler": "~7.2.1",
    "@angular/core": "~7.2.1",
    "@angular/forms": "~7.2.1",
    "@angular/http": "~7.2.1",
    "@angular/material": "^7.3.0",
    "@angular/platform-browser": "~7.2.1",
    "@angular/platform-browser-dynamic": "~7.2.1",
    "@angular/router": "~7.2.1",
    "core-js": "^2.6.3",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "ngx-swiper-wrapper": "^7.2.1",
    "papaparse": "~4.6.3",
    "rxjs": "~6.3.3",
    "typeface-lato": "0.0.54",
    "typeface-montserrat": "0.0.54",
    "videogular2": "^6.4.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.2",
    "@angular/cli": "^7.2.2",
    "@angular/compiler-cli": "~7.2.1",
    "@angular/language-service": "~7.2.1",
    "@compodoc/compodoc": "^1.1.8",
    "@types/core-js": "^2.5.0",
    "@types/jasmine": "~3.3.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.120",
    "@types/node": "~10.12.18",
    "@types/papaparse": "^4.5.8",
    "@types/swiper": "^4.4.1",
    "async": "^2.6.1",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nativefier": "^7.6.10",
    "ng-mocks": "~7.5.0",
    "protractor": "~5.4.0",
    "shelljs": "^0.8.3",
    "ts-node": "~8.0.1",
    "tslint": "~5.12.1",
    "typescript": "~3.2.4"
  }
}
